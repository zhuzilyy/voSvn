<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/title"
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_top_bar"
        android:background="@color/white"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:paddingLeft="@dimen/x12">

        <ImageView
            android:id="@+id/iv_back"

            android:layout_width="@dimen/x20"
            android:layout_height="@dimen/x20"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:scaleType="centerInside"
            android:src="@mipmap/back_" />

        <View
            android:id="@+id/view_temp"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_marginBottom="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="10dp"
            android:visibility="gone"
            android:layout_toRightOf="@id/iv_back"
            android:background="#14191A" />

        <TextView
            android:id="@+id/web_title_"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
           android:layout_centerInParent="true"
            android:text="帮助与反馈"
            android:textColor="@color/black"
            android:textSize="18sp" />
        <TextView
            android:visibility="gone"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:text="添加朋友"
            android:layout_marginRight="@dimen/x12"
            android:textColor="#ffffff"
            android:textSize="@dimen/x12" />


    </RelativeLayout>
    <View
        android:layout_below="@+id/title"
        android:id="@+id/flag"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/light_gray5"
        />
    <RelativeLayout
        android:layout_below="@+id/flag"
        android:id="@+id/re_function"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:background="@drawable/fx_btn_list_item_bg"
        android:clickable="true"
        android:paddingLeft="12dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/iv_yaoyiyao"
            android:text="功能介绍"
            android:textColor="#353535"
            android:textSize="16sp"
            android:id="@+id/textView7" />

        <ImageView
            android:id="@+id/iv_yaoyiyao"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp"
            android:src="@mipmap/function_icon"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="12dp" />
    </RelativeLayout>
    <View
        android:id="@+id/flag01"
        android:layout_below="@+id/re_function"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/light_gray4" />

    <!--关于vo-->
    <RelativeLayout
        android:layout_below="@+id/flag01"
        android:id="@+id/re_feedback"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:background="@drawable/fx_btn_list_item_bg"
        android:clickable="true"
        android:paddingLeft="12dp">

        <ImageView
            android:id="@+id/iv_aboutvo"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp"
            android:src="@mipmap/feedback_icon" />

        <TextView
            android:id="@+id/aboutTvVo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@id/iv_aboutvo"
            android:text="反馈"
            android:textColor="#353535"
            android:textSize="16sp" />

        <TextView
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x5"
            android:layout_marginTop="@dimen/x6"
            android:layout_toRightOf="@+id/aboutTvVo"
            android:background="@drawable/blue_bg"
            android:paddingBottom="@dimen/x2"
            android:paddingLeft="@dimen/x10"
            android:paddingRight="@dimen/x10"
            android:paddingTop="@dimen/x2"
            android:text="更新"
            android:textColor="@color/white"
            android:textSize="@dimen/recall_size_12" />
        <!--<View-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="0.1dp"-->
        <!--android:layout_alignParentBottom="true"-->
        <!--android:layout_marginRight="12dp"-->
        <!--android:background="#dadada" />-->
    </RelativeLayout>


</RelativeLayout>